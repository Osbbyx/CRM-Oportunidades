                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#A09393"
                    android:drawableLeft="@android:drawable/ic_delete"
                    android:onClick="concluirc"
                    android:text="Concluir"
                    android:textColor="@android:color/background_light"
                    android:textStyle="bold" />
            </LinearLayout>

            <Button
                android:id="@+id/d"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/holo_orange_dark"
                android:drawableLeft="@drawable/ic_bubble_chart_black_24dp"
                android:text="Empresa empanaditas"
                android:textColor="@android:color/background_light" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/d1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#6F6666"
                    android:drawableLeft="@android:drawable/ic_media_ff"
                    android:onClick="avanzar"
                    android:text="@string/avanzar"
                    android:textColor="#fff" />

                <Button
                    android:id="@+id/d2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#A09393"
                    android:drawableLeft="@android:drawable/ic_delete"
                    android:onClick="concluird"
                    android:text="Concluir"
                    android:textColor="@android:color/background_light"
                    android:textStyle="bold" />
            </LinearLayout>

            <Button
                android:id="@+id/e"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/holo_blue_light"
                android:drawableLeft="@drawable/ic_bubble_chart_black_24dp"
                android:text="Empresa rustica"
                android:textColor="@android:color/background_light" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/e1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#6F6666"
                    android:drawableLeft="@android:drawable/ic_media_ff"
                    android:onClick="avanzar"
                    android:text="@string/avanzar"
                    android:textColor="#fff" />

                <Button
                    android:id="@+id/e2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#A09393"
                    android:drawableLeft="@android:drawable/ic_delete"
                    android:onClick="concluire"
                    android:text="Concluir"
                    android:textColor="@android:color/background_light"
                    android:textStyle="bold" />
            </LinearLayout>

            <Button
                android:id="@+id/f"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/holo_blue_bright"
                android:drawableLeft="@drawable/ic_bubble_chart_black_24dp"
                android:text="Empresa jueguitos"
                android:textColor="@android:color/background_light" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/f1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#6F6666"
                    android:drawableLeft="@android:drawable/ic_media_ff"
                    android:onClick="avanzar"
                    android:text="@string/avanzar"
                    android:textColor="#fff" />

                <Button
                    android:id="@+id/f2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#A09393"
                    android:drawableLeft="@android:drawable/ic_delete"
                    android:onClick="concluirf"
                    android:text="Concluir"
                    android:textColor="@android:color/background_light"
                    android:textStyle="bold" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</RelativeLayout>